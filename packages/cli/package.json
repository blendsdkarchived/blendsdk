        "clean": "rimraf .build dist",
        "tslint": "tslint --config ../../tslint.json --project ./tsconfig.json"
    },
    "bin": {
        "blendsdk": "dist/index.js"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "keywords": [
        "typescript",
        "framework",
        "library",
        "javascript",
        "html5",
        "UI",
        "components"
