    "dependencies": {
        "@types/ejs": "^2.6.3",
        "@types/glob": "^7.1.1",
        "@types/mkdirp": "^0.5.2",
        "@types/yargs": "^13.0.0",
        "chalk": "^2.4.2",
        "concurrently": "^4.1.0",
        "ejs": "^2.6.1",
        "glob": "^7.1.3",
        "mkdirp": "^0.5.1",
        "rimraf": "^2.6.3",
        "rollup": "^1.10.1",
        "rollup-plugin-commonjs": "^9.3.4",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-node-globals": "^1.4.0",
        "rollup-plugin-node-resolve": "^4.2.3",
        "rollup-plugin-node-resolve-magic": "^0.2.11",
        "rollup-plugin-sourcemaps": "^0.4.2",
        "rollup-plugin-uglify": "^6.0.2",
        "shelljs": "^0.8.3",
