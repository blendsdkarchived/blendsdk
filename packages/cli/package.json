    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/blendsdk/blendsdk"
    },
    "scripts": {
        "watch": "tsc -w",
        "build": "npm run clean && npm run tslint && npm run build:lib",
