        "@blendsdk/application": "^0.0.80",
        "@blendsdk/blendrunner": "^0.0.80",
        "@blendsdk/browser": "^0.0.80",
        "@blendsdk/core": "^0.0.80",
        "@blendsdk/css": "^0.0.80",
        "@blendsdk/deviceinfo": "^0.0.80",
        "@blendsdk/dom": "^0.0.80",
        "@blendsdk/extensions": "^0.0.80",
        "@blendsdk/forms": "^0.0.80",
        "@blendsdk/icon": "^0.0.80",
        "@blendsdk/mvc": "^0.0.80",
        "@blendsdk/task": "^0.0.80",
        "@blendsdk/ui": "^0.0.80",
        "@blendsdk/uistack": "^0.0.80",
        "@blendsdk/viewrouter": "^0.0.80",
