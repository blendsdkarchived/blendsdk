        "clean": "rimraf .build dist",
        "tslint": "tslint --config ../../tslint.json --project ./tsconfig.json"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "keywords": [
        "typescript",
        "framework",
        "library",
        "javascript",
        "html5",
        "UI",
        "components"
    ],
    "dependencies": {
