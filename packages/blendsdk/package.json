{
    "name": "@blendsdk/blendsdk",
    "version": "0.0.30",
    "description": "BlendSDK HTML5 Web Application Framework for TypeScript",
    "author": "Gevik Babakhani <gevik@blendjs.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/blendsdk/blendsdk"
    },
    "scripts": {
        "watch": "tsc -w",
        "build": "npm run clean && npm run tslint && npm run build:lib",
        "build:lib": "tsc -p ./",
        "clean": "rimraf .build dist",
        "tslint": "tslint --config ../../tslint.json --project ./tsconfig.json",
        "rollup": "rollup --config ./rollup.config.json"
    },
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "browser": "dist/index.umd.js",
    "types": "dist/index.d.ts",
    "keywords": [
        "typescript",
        "framework",
        "library",
        "javascript",
        "html5",
        "UI",
        "components"
    ],
    "gitHead": "3cf0c717504e5e6ee768f9423eec41a997662df9",
    "devDependencies": {
        "rimraf": "^2.6.3",
        "tslint": "^5.12.1",
        "typescript": "^3.3.3"
    }
}
