{
    "defaultSeverity": "error",
    "linterOptions": {
        "exclude": ["**/node_modules/**/*", "**/dist/**/*"]
    },
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "trailing-comma": false,
        "indent": false,
        "arrow-parens": false,
        "one-variable-per-declaration": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
